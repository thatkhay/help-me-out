 
{
  "manifest_version": 3,
  "name": "Screen Recording Extension",
  "version": "1.0",
  "description": "Record your screen",
  "permissions": ["activeTab", "desktopCapture"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
 
}
